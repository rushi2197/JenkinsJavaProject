pipeline {
    agent any
    stages {
        stage('Checkout Other Repo') {
            steps {
                git branch: 'master',
                    url: 'https://github.com/rushi2197/myCodes_Practice.git'
            }
        }
        stage('List Files') {
            steps {
                sh 'ls -la'
            }
        }
    }
}
